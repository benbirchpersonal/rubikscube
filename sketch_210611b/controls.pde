
void keyPressed(){
  switch(key){
    case ('1'):
    turnX(1);
    break;
    
    case ('2'):
    turnX(-1);
    break;
    
    case('3'):
    turnY(1);
    break;
    
    case('4'):
      turnY(-1);
      break;
      
      case('5'):
      turnZ(1);
      break;
      
      case('6'):
      turnZ(-1);
      break;
      
  }
}
